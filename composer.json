{
  "name": "grazianilog/sdk-logs-logmanager",
  "description": "Um pacote para gerenciar logs das apps desenvolvidas pela logmanager.",
  "authors": [
    {
      "name": "Graziani",
      "email": "graziani@logmanager.com.br"
    },
    {
      "name": "Jesiel Nunes",
      "email": "jesiel@logmanager.com.br"
    }
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "jenssegers/mongodb": "3.8.*"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "LogManager\\sdk-logs\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LogManager\\SdkLogs\\SdkLogsServiceProvider"
      ],
      "aliases": {
        "SDKLogs": "LogManager\\SdkLogs\\Facades\\SDKLogs"
      }
    }
  }
}
